<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane prefHeight="276.0" prefWidth="376.0" style="-fx-background-color: #84A169"
      xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.jigsaw.client.viewControllers.ResultController">


    <VBox alignment="CENTER" layoutX="16.0" layoutY="16.0" prefHeight="245.0"
          prefWidth="348.0" spacing="10">
        <Label style="-fx-font-family: Arial; -fx-font-weight: Bold; -fx-font-size: 20"
               text="Results"/>
        <Label fx:id="resultPlayerNameLabel"
               style="-fx-font-family: Arial; -fx-font-weight: Bold; -fx-font-size: 13"/>
        <Label fx:id="resultLabel" style="-fx-font-family: Arial; -fx-font-size: 12"/>
        <Button onAction="#onStartButtonClick" text="Start"/>
        <Button onAction="#onTopButtonClick" text="Top players"/>
        <Button mnemonicParsing="false" onAction="#onQuitButtonClick" text="Quit"/>
    </VBox>

</Pane>
